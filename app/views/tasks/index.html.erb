<h1>ToDo:</h1>
<% @tasks.each do |task| %>
  <p>
    <strong><%= link_to task.title, task_path(task) %></strong>
    | <%= link_to "Delete", task,
        method: :delete,
        data: { confirm: "Are you sure You want to delte task '#{task.title}'" }%>
  </p>
<% end %>
<%= link_to "New task", new_task_path %>
